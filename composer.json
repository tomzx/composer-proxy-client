	"type": "library",
	"keywords": [
		"composer",
		"proxy",
		"client"
	],
	"homepage": "https://github.com/tomzx/composer-proxy-client",
	"license": "MIT",
	"authors": [
		{
			"name": "Tom Rochette",
			"email": "tom@tomrochette.com",
			"homepage": "http://www.tomrochette.com"
		}
	],
	"support": {
		"issues": "https://github.com/tomzx/composer-proxy-client/issues"
	},
